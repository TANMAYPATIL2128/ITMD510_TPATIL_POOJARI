<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BookingController">
   <children>
      <Label layoutX="141.0" layoutY="35.0" prefHeight="35.0" prefWidth="219.0" text="BOOKING TICKETS" textFill="#d00f0f">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="402.0" layoutY="417.0" mnemonicParsing="false" onAction="#back" text="BACK">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="proceed" layoutX="205.0" layoutY="301.0" mnemonicParsing="false" onAction="#proceed" text="PROCEED">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <ComboBox fx:id="title" layoutX="124.0" layoutY="146.0" prefHeight="25.0" prefWidth="238.0" promptText="SELECT THE MOVIE" />
   </children>
</AnchorPane>
