<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomeController">
   <children>
      <Label layoutX="177.0" layoutY="54.0" prefHeight="32.0" prefWidth="148.0" text="HOME PAGE" textFill="#d71212">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="booking" layoutX="211.0" layoutY="151.0" mnemonicParsing="false" onAction="#bookingMovie" text="BOOKING">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="movielist" layoutX="204.0" layoutY="241.0" mnemonicParsing="false" onAction="#movieList" text="MOVIE LIST">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="logout" layoutX="212.0" layoutY="324.0" mnemonicParsing="false" onAction="#logout" text="LOG OUT">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
